{
  "name": "commonsku",
  "version": "1.0.0",
  "description": "A TypeScript-based CLI tool for interacting with the GitHub REST API",
  "main": "build/index.js",
  "type": "module",
  "scripts": {
    "start": "node build/index.js",
    "build": "tsc",
    "dev": "node --loader ts-node/esm src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rm -rf build",
    "lint": "eslint . --fix",
    "format": "prettier --write ."
  },
  "keywords": [
    "github",
    "api",
    "cli",
    "typescript"
  ],
  "author": "Yoandry Collazo",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.10",
    "@types/source-map-support": "^0",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.5.1",
    "jest": "^30.0.4",
    "prettier": "^3.0.0",
    "source-map": "^0.7.4",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@octokit/core": "^7.0.2",
    "@octokit/rest": "^22.0.0",
    "axios": "^1.6.0",
    "dotenv": "^16.0.0",
    "octokit": "^5.0.3"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
