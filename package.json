{
  "name": "github-api-project",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/index.js",
    "dev": "ts-node --loader ts-node/esm src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:check": "eslint . --max-warnings 0",
    "type-check": "tsc --noEmit",
    "format": "yarn lint:fix",
    "check": "yarn type-check && yarn lint:check && yarn test"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^24.0.7",
    "@yarnpkg/pnpify": "^4.1.5",
    "eslint": "^9.30.0",
    "jest": "^29.7.0",
    "jest-core": "^0.0.0",
    "jest-util": "^30.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.35.0"
  },
  "packageManager": "yarn@4.9.1"
}
